{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "29f73f44-bb92-4a0c-b07e-681f5db10b9d"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-localization": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-localization",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            },
            "pt-br": {
              "localize": ["pt-br"]
            },
            "en": {
              "localize": ["en"]
            },
            "fr": {
              "localize": ["fr"]
            },
            "de": {
              "localize": ["de"]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-localization:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-localization:build:production"
            },
            "pt-br": {
              "browserTarget": "angular-localization:build:pt-br"
            },
            "en": {
              "browserTarget": "angular-localization:build:en"
            },
            "fr": {
              "browserTarget": "angular-localization:build:fr"
            },
            "de": {
              "browserTarget": "angular-localization:build:de"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-localization:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angular-localization:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-localization:serve:production"
            }
          }
        }
      },
      "i18n": {
        "locales": {
          "pt-br": "src/translate/messages.pt-br.xlf",
          "en": "src/translate/messages.en.xlf",
          "fr": "src/translate/messages.fr.xlf",
          "de": "src/translate/messages.de.xlf"
        }
      }
    }
  },
  "defaultProject": "angular-localization"
}
